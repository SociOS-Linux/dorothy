{
  "css.validate": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line-thin",
  "editor.detectIndentation": false,
  "editor.fontFamily": "FiraCode-Retina, Fira Code, Fira Mono, Monoid-Retina, Monoid, Hasklig, Source Code Pro, IBM Plex Mono, Hack, Monaco, PT Mono, Andale Mono, Menlo",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.insertSpaces": false,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.roundedSelection": true,
  "editor.tabSize": 3,
  "editor.wordWrap": "on",
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.run": "onType",
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "go.gopath": "~/go",
  "html.format.enable": true,
  "javascript.format.enable": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.format.enable": true,
  "lit-html.format.enabled": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.useTabs": true,
  "python.linting.pylintPath": "/usr/local/bin/pylint",
  "search.useIgnoreFiles": true,
  "terminal.integrated.fontFamily": "Hasklig, Source Code Pro, IBM Plex Mono, Hack, Monaco, Fira Code, Fira Mono, PT Mono, Andale Mono, Menlo",
  "terminal.integrated.fontWeightBold": "600",
  "terminal.integrated.rendererType": "dom",
  "typescript.format.enable": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "vsicons.projectDetection.disableDetect": true,
  "window.closeWhenEmpty": true,
  "window.nativeTabs": true,
  "window.titleBarStyle": "native",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.tabCloseButton": "left",
  "workbench.fontAliasing": "antialiased",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.fullScreen": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "zenMode.restore": true,
  "files.associations": {
    "*.lsrules": "json",
    "*.pine": "go"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.vscode": false,
    "**/database": true,
    "**/database.json": true,
    "**/node_modules": true,
    "**/pnp": true
  },
  "[plaintext]": {
    "editor.folding": false,
    "editor.fontFamily": "Ubuntu, Lato, Roboto, Spectral",
    "editor.fontSize": 16,
    "editor.insertSpaces": true,
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.tabSize": 4
  },
  "[mdx]": {
    "editor.folding": false,
    "editor.fontFamily": "Ubuntu, Lato, Roboto, Spectral",
    "editor.fontSize": 16,
    "editor.insertSpaces": true,
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.tabSize": 4
  },
  "markdownlint.config": {
    "MD007": {
      "indent": 4
    },
    "MD012": false,
    "MD022": false,
    "MD030": false,
    "MD032": false,
    "MD034": false
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "ignorePattern": "**/*.d.ts"
  },
  "theme-by-language.themes": {
    "*": "An Old Hope",
    "plaintext": "Adapta Nokto",
    "mdx": "Adapta Nokto"
  },
  "workbench.colorTheme": "An Old Hope",
  "window.zoomLevel": 0,
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "go.useLanguageServer": true
}
