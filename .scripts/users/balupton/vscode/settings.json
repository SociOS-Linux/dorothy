{
	"css.validate": true,
	"editor.cursorBlinking": "solid",
	"editor.cursorStyle": "line-thin",
	"editor.detectIndentation": false,
	"editor.folding": false,
	"editor.fontFamily": "FiraCode-Retina, Fira Code, Fira Mono, Monoid-Retina, Monoid, Hasklig, Source Code Pro, IBM Plex Mono, Hack, Monaco, PT Mono, Andale Mono, Menlo",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.renderControlCharacters": true,
	"editor.renderIndentGuides": true,
	"editor.renderWhitespace": "all",
	"editor.roundedSelection": true,
	"editor.tabSize": 3,
	"editor.wordWrap": "on",
	"eslint.alwaysShowStatus": true,
	"eslint.autoFixOnSave": true,
	"eslint.enable": true,
	"eslint.run": "onType",
	"eslint.validate": ["javascript", "javascriptreact", "typescript"],
	"explorer.confirmDragAndDrop": false,
	"extensions.autoUpdate": true,
	"files.trimTrailingWhitespace": true,
	"flow.enabled": false,
	"flow.useNPMPackagedFlow": true,
	"git.confirmSync": false,
	"go.gopath": "~/go",
	"javascript.format.enable": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.implicitProjectConfig.checkJs": true,
	"javascript.implicitProjectConfig.experimentalDecorators": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"materialTheme.showReloadNotification": false,
	"prettier.semi": false,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"python.linting.pylintPath": "/usr/local/bin/pylint",
	"search.useIgnoreFiles": true,
	"terminal.integrated.fontFamily": "Hasklig, Source Code Pro, IBM Plex Mono, Hack, Monaco, Fira Code, Fira Mono, PT Mono, Andale Mono, Menlo",
	"terminal.integrated.fontWeightBold": "600",
	"terminal.integrated.rendererType": "dom",
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.tsdk": "./node_modules/typescript/lib",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.validate.enable": true,
	"window.closeWhenEmpty": true,
	"window.nativeTabs": true,
	"window.titleBarStyle": "native",
	"workbench.colorTheme": "GitHub Plus",
	"workbench.editor.closeOnFileDelete": false,
	"workbench.editor.tabCloseButton": "left",
	"workbench.fontAliasing": "antialiased",
	"workbench.iconTheme": "dobri-simple-icons",
	"workbench.startupEditor": "newUntitledFile",
	"zenMode.fullScreen": false,
	"zenMode.hideStatusBar": false,
	"zenMode.hideTabs": false,
	"zenMode.restore": true,
	"files.associations": {
		"*.lsrules": "json",
		"*.pine": "go"
	},
	"files.exclude": {
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/.vscode": false,
		"**/database": true,
		"**/database.json": true,
		"**/node_modules": true,
		"**/yarn.lock": true
	},
	"[markdown]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"markdownlint.config": {
		"MD007": {
			"indent": 4
		},
		"MD012": false,
		"MD022": false,
		"MD030": false,
		"MD032": false,
		"MD034": false
	},
	"eslint.options": {
		"ignorePattern": "**/*.d.ts"
	},
	"vsicons.associations.folders": [
		{
			"icon": "www",
			"extensions": ["static", "client", "hooks", "pages", "components"],
			"format": "png"
		}
	]
}
