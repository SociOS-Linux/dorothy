#!/usr/bin/env bash
source "$HOME/.scripts/sources/strict.bash"

r="$(sha256sum "$1")"
h="${r%  *}"

# Clone a list of repositories
for a in "${@:2}"; do
	rr="$(sha256sum "$a")"
	hh="${rr%  *}"
	if test "$hh" != "$h"; then
		stderr echo "not ok"
		echo "$h"
		echo "$hh"
		exit 1
	fi
done

echo "ok"
exit 0