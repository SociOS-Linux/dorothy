#!/usr/bin/env bash
set -e

# iOS Simulator
app=$(getapp Xcode-beta.app)
if is_dir "$app"; then
	open "$app/Contents/Developer/Applications/Simulator.app"
else
	app=$(getapp Xcode.app)
	if is_dir "$app"; then
		open "$app/Contents/Developer/Applications/Simulator.app"
	else
		echo "Xcode is not installed"
	fi
fi