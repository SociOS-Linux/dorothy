#!/usr/bin/env bash
source "$HOME/.scripts/sources/strict.bash"

echo -e '\nInstalling fonts...'
brew cask install --force \
	font-cantarell \
	font-fira-code \
	font-fira-mono \
	font-hack \
	font-hasklig \
	font-ibm-plex \
	font-inter \
	font-lato \
	font-maven-pro \
	font-monoid \
	font-montserrat \
	font-open-sans \
	font-oxygen \
	font-oxygen-mono \
	font-roboto \
	font-roboto-mono \
	font-source-code-pro \
	font-ubuntu \
	;
# failed to install: font-andale-mono font-droid-sans