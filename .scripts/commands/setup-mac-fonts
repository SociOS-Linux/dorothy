#!/usr/bin/env bash
set -e

echo -e "\nInstalling fonts..."
brew cask install \
	font-cantarell \
	font-fira-code \
	font-fira-mono \
	font-hack \
	font-hasklig \
	font-ibm-plex \
	font-lato \
	font-maven-pro \
	font-monoid \
	font-montserrat \
	font-open-sans \
	font-oxygen \
	font-oxygen-mono \
	font-roboto \
	font-roboto-mono \
	font-source-code-pro \
	font-ubuntu \
	;
